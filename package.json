{
  "name": "typescript-typelevel",
  "version": "0.8.1",
  "description": "Lift your code to the next level.",
  "keywords": [
    "type level programming",
    "typescript",
    "utility types"
  ],
  "files": [
    "lib",
    "src"
  ],
  "types": "lib/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "prepare": "npm run clean && npm run build && npm run lint",
